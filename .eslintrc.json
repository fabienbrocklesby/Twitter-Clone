{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "import/extensions": "off",
      "camelcase": "off",
      "consistent-return": "off",
      "no-param-reassign": "off"
    }
}
